    "base64-arraybuffer": "^1.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "i18next-resources-to-backend": "^1.2.1",
    "lucide-react": "^0.503.0",
    "motion": "^12.16.0",
    "next": "15.2.4",
    "next-i18n-router": "^5.5.2",
    "next-i18next": "^15.4.2",
    "next-intl": "^4.1.0",
